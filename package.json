{
  "name": "csm_web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development ./csm_web/frontend/src/index.js --output ./csm_web/frontend/static/frontend/main.js",
    "build:js": "webpack --mode production ./csm_web/frontend/src/index.js --output ./csm_web/frontend/static/frontend/main.js",
    "build:css": "uglifycss ./csm_web/frontend/static/frontend/css/style.css --output ./csm_web/frontend/static/frontend/css/style.min.css",
    "build": "npm run build:js && npm run build:css",
    "watch": "nodemon -d 3 -w ./csm_web/frontend/src/components -w ./csm_web/frontend/src/utils --exec 'npm run dev'"
  },
  "heroku-run-build-script": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csmberkeley/csm_web.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csmberkeley/csm_web/issues"
  },
  "homepage": "https://github.com/csmberkeley/csm_web#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@svgr/webpack": "^4.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^7.0.0-beta.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "lodash-webpack-plugin": "^0.11.5",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "uglifycss": "0.0.29",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "react-router-dom": "^5.2.0"
  }
}
